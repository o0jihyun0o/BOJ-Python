    "while(True):\n",
    "    word = input()\n",
    "    reverse_word = word[::-1]\n",
    "    \n",
    "    if word == '0':\n",
    "        break\n",
    "    else:\n",
    "        if word == reverse_word:\n",
    "            print('yes')\n",
    "        else:\n",
    "            print('no')"
